{
  "name": "11ty-template",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build:css": "postcss src/assets/sass/styles.scss -o src/assets/css/styles.css",
    "build": "npm run build:css && eleventy",
    "serve": "npm run build:css && eleventy --serve",
    "watch:css": "postcss src/assets/sass/styles.scss -o src/assets/css/styles.css --watch",
    "watch": "npm-run-all --parallel watch:css serve"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@quasibit/eleventy-plugin-sitemap": "^2.2.0",
    "@sherby/eleventy-plugin-files-minifier": "^1.1.1",
    "autoprefixer": "^10.4.20",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.42",
    "postcss-cli": "^11.0.0",
    "sass": "^1.77.8",
    "tailwindcss": "^3.4.10"
  }
}
